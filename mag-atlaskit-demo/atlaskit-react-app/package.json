{
  "name": "mag-atlaskit-demo",
  "version": "0.0.1",
  "description": "Get your ADG3 on with this sample project using Atlaskit React components",
  "scripts": {
    "start": "react-scripts start",
    "build": "node ./scripts/build-non-split.js  && npm run clean:js && npm run copy:js  && npm run clean:css && npm run copy:css",
    "build-old": "react-scripts build && npm run clean:js && npm run copy:js  && npm run clean:css && npm run copy:css",
    "clean:js": "rm -f ../light-modules/atlaskit/webresources/static/js/*",
    "clean:css": "rm -f ../light-modules/atlaskit/webresources/static/css/*",
    "copy:js": "cp build/static/js/* ../light-modules/atlaskit/webresources/static/js ",
    "copy:css": "cp build/static/css/* ../light-modules/atlaskit/webresources/static/css ",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "keywords": [],
  "author": "Atlassian & Magnolia",
  "license": "ISC",
  "dependencies": {
    "magnolia-react-area51": "file:../../magnolia-react-area51",
    "@atlaskit/avatar": "^14.0.9",
    "@atlaskit/button": "^9.0.7",
    "@atlaskit/css-reset": "^3.0.1",
    "@atlaskit/dropdown-menu": "^6.1.10",
    "@atlaskit/flag": "^9.0.7",
    "@atlaskit/icon": "^13.4.0",
    "@atlaskit/inline-edit": "^7.0.7",
    "@atlaskit/modal-dialog": "^6.0.10",
    "@atlaskit/navigation": "^33.1.7",
    "@atlaskit/page": "^8.0.4",
    "@atlaskit/theme": "^6.0.1",
    "prop-types": "^15.6.0",
    "styled-components": "^3.2.6",
    "react": "^16.4.2",
    "react-dom": "^16.2.4",
    "react-router": "^3.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "react-scripts": "^1.1.1",
    "surge": "^0.20.1",
    "rewire": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/magnolia-community/react-area51-alpha/"
  }
}
